<UserControl x:Class="Alphora.Dataphor.Frontend.Client.Silverlight.ConnectApplication"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    >
    <StackPanel x:Name="LayoutRoot">
    	<StackPanel HorizontalAlignment="Right" Orientation="Horizontal" Margin="0,4,0,0">
    		<Button x:Name="BackButton" Content="Back" Click="BackClicked" Style="{StaticResource ButtonStyle}" />
    		<Button x:Name="StartButton" Content="Start" Click="StartButton_Click" Style="{StaticResource ButtonStyle}" />
    	</StackPanel>
    </StackPanel>
</UserControl>
