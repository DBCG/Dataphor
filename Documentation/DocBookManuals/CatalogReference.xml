				<sect3 id="D4SystemDataTypeSystemTimeSpan"> 
					 <title>System.TimeSpan</title> 
					 <para/> 
						  <sect4> 
								<title>Syntax</title> 
								<para> 
									 <programlisting>domain System.TimeSpan is { System.Alpha }
{
	representation Ticks { Ticks : Long },
	representation Milliseconds { Milliseconds : Decimal },
	representation Seconds { Seconds : Decimal },
	representation Minutes { Minutes : Decimal },
	representation Hours { Hours : Decimal },
	representation Days { Days : Decimal },
	representation TimeSpan
	{
		DaysPart : Integer,
		HoursPart : Integer,
		MinutesPart : Integer,
		SecondsPart : Integer,
		MillisecondsPart : Integer
	}
};
		</programlisting> </para> 
						  </sect4> 
				</sect3> 
				<sect3 id="D4SystemDataTypeSystemDateTime"> 
					 <title>System.DateTime</title> 
					 <para/> 
						  <sect4> 
								<title>Syntax</title> 
								<para> 
									 <programlisting>domain System.DateTime is { System.TimeSpan }
{
	representation DateTime
	{
		YearsPart : Integer,
		MonthsPart : Integer,
		DaysPart : Integer,
		HoursPart : Integer,
		MinutesPart : Integer,
		SecondsPart : Integer,
		MillisecondsPart : Integer
	},
	constraint DateTimeValue Value.Ticks &gt;= 0
};
		</programlisting> </para> 
						  </sect4> 
				</sect3> 
