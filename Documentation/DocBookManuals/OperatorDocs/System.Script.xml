<sect2> 
	 <sect2info><date>Tuesday, April 06, 2004 14:25:56</date>
	 </sect2info>
	 <title>System.Script</title> 
	 <para></para> 
	 <bridgehead renderas="sect3">Remarks</bridgehead> 
	 <para>Returns a string representing the creation statement needed to create
		  the specified object.</para> 
	 <example>
		  <title>Example</title>
		  <programlisting>select TableDee add { Script(Name("Employee")) script};</programlisting> 
		  <programlisting>script                                                                                                                                                                        
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------- 
create table Int.Employee in System.Temp
{
	ID : System.Integer tags {DAE.StaticByteSize = "4"}, 
	Name : System.String tags {DAE.StaticByteSize = "44"}, 
	key{ID}
	
}</programlisting>
	 </example> 
</sect2> 
