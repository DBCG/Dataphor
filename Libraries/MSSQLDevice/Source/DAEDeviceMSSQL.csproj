<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <Deterministic>false</Deterministic>
    <AssemblyName>AlphoraMSSQLDevice</AssemblyName>
    <RootNamespace></RootNamespace>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>SIGNASSEMBLIES</DefineConstants>
    <OutputPath>..\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>..\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Content Remove="SystemCatalog.d4" />
    <Content Remove="SystemCatalog.sql" />
    <Content Remove="Device.MSSQL\MSSQLfunction.sql" />
    <Content Remove="Device.MSSQL\MSSQLfunction.tsql" />
    <Content Remove="Language.TSQL\BNF.txt" />
    <Content Remove="Language.TSQL\UnsupportedOperators.txt" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="SystemCatalog.d4" />
    <EmbeddedResource Include="SystemCatalog.sql" />
    <EmbeddedResource Include="Device.MSSQL\MSSQLfunction.sql" />
    <EmbeddedResource Include="Device.MSSQL\MSSQLfunction.tsql" />
    <EmbeddedResource Include="Language.TSQL\BNF.txt" />
    <EmbeddedResource Include="Language.TSQL\UnsupportedOperators.txt" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\Dataphor\Base\Base.csproj" />
    <ProjectReference Include="..\..\..\Dataphor\Common\Common.csproj" />
    <ProjectReference Include="..\..\..\Dataphor\Server\Server.csproj" />
    <ProjectReference Include="..\..\Connection\Source\DAE.Connection.csproj" />
    <ProjectReference Include="..\..\SQLDevice\Source\DAE.Device.SQL.csproj" />
  </ItemGroup>
</Project>
